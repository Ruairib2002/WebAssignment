<h1>Edit Profile</h1>
<%= form_for(current_user, url: profile_path) do |f| %>
<%= f.label :full_name %>
<%= f.text_field :full_name %>
<%= f.label :email %>
<%= f.email_field :email %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.submit 'Save Changes' %>
<% end %>